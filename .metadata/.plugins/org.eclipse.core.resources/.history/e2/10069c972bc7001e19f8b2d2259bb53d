package ejerciciosPoo;

public class Fibonacci {

	
	public static int Fibo(int N) {
		
	if (N==0) 
		return 0;
		
	 else if(N==1) 
		return 1;
	
		else 
		return Fibo(N-1)+Fibo(N-2);		
	}
	
	
	public static void main(String[] args) {
		
		System.out.println("Fibonacci hasta 100: ");
		
		for (int i = 0; i < 10; i++) {
			
			Fibo(10);
			
			System.out.print(i+",");
			
		}

	}

}
