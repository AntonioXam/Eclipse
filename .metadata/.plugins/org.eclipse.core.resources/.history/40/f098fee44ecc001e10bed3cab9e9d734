package Boletines;

public class Matriztest {

	public static void main(String[] args) {
		
		
		Matriz m=new Matriz(4);
		m.generarValores();
		System.out.println(m.toString());
		
		System.out.println("Mayor número: "+m.mayorValor());
		System.out.println("Menor número: "+m.menorValor());
	
		m.setPosicion(1, 2, 5);
		System.out.println(m.toString());

	}

}
